<?xml version="1.0" encoding="UTF-8" ?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
  <title>Thysbelon</title>
  <link rel="self" href="https://thysbelon.github.io/atom-en.xml"/>
  <link href="https://thysbelon.github.io/Home"/>
  <updated>2022-03-08T17:00:00Z</updated>
  <subtitle>My personal blog where I post about whatever interests me</subtitle>
  <id>thysbelon-en</id>
  <icon>https://thysbelon.github.io/thysbelonicon.svg</icon>
  <logo>https://thysbelon.github.io/ThysbelonTitle.svg</logo>
  <author>
    <name>Thysbelon</name>
  </author>
{% assign posts = site.posts | where: "layout", "post-en" %}{% for post in posts %}{% comment %}removed "limit:10" because of https://twitter.com/Seglegs/status/1419709872904581123 "Because nobody outside of techies has ever heard of RSS, we get these awful "newsletters" which are opaque (no back issues), non-customizable (can't combine NY Times with New Yorker), and mine your email for marketing."{% endcomment %}
  <entry>
    <title>{{ post.title }}</title>
	<id>{{ post.id }}</id>
    <link href="{{ post.url }}"/>
    <summary type="html">{% assign outputpost = post.excerpt %}{% assign outputpost = outputpost | replace: "<img src=/emoji/", "protectemoji" %}{% assign imgalt = outputpost | split:'alt="' %}{% assign imgalt2 = imgalt[1] | split:'">' | first %}{% assign imgsrc = outputpost | split:'src=' %}{% assign imgsrc2 = imgsrc[1] | split:' ' | first %}{% if imgsrc2 contains "." %}<![CDATA[<a href="{{ post.url | url_encode | replace: "%2F","/" }}"><img alt="{{ imgalt2 }}" src="{{ imgsrc2 | prepend: "/../" | prepend: post.url | url_encode | replace: "%2F","/" }}" style="max-height:80vh;float:right" /></a>]]>{% endif %}{% assign outputpost = outputpost | strip_html | truncatewords: 50 | replace: "&ldquo;", '"' | replace: "&rdquo;",'"' %}{{ outputpost | replace: "protectemoji", "<img src=/emoji/" | remove: "&gt;" | remove: "&lt;" | replace: "<","&lt;" | replace: ">","&gt;" | replace: "&","&amp;" }}</summary>
	<updated>{{ post.date | date: "%Y-%m-%dT17:00:00Z"}}</updated>
	<published>{{ post.date | date: "%Y-%m-%dT17:00:00Z"}}</published>
  </entry>
{% endfor %}
</feed>